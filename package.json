  "overrides": {
    "@web/test-runner-playwright": {
      "playwright": "1.54.1"
    }
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:scripts",
        "build:ts"
      ]
    },
    "build:ts": {
      "command": "tsc --pretty",
      "files": [
        "tsconfig.json",
        "**/*.ts",
        "!**/*.d.ts",
        "!**/*-styles.ts",
        "!catalog/",
        "!scripts/",
        "!dist/"
      ],
      "output": [
        "dist/**/*.js",
        "dist/**/*.js.map",
        "dist/**/*.d.ts"
      ],
      "clean": "if-file-deleted",
      "dependencies": []
    },
    "build:scripts": {
      "command": "tsc --project scripts/tsconfig.json --pretty",
      "files": [
        "scripts/tsconfig.json",
        "scripts/*.ts",
        "!**/*.d.ts",
        "!**/*-styles.ts"
      ],
      "output": [
        "./dist/scripts/**/*.js",
        "./dist/scripts/**/*.js.map",
        "./dist/scripts/**/*.d.ts"
      ],
      "clean": "if-file-deleted"
    }
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "eslint"
    ]
  },
  "customElements": "custom-elements.json"
}
